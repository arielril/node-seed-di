{
  "name": "node-seed-di",
  "version": "1.0.0",
  "description": "Seed project with Dependency Injection!",
  "main": "src/app.js",
  "scripts": {
    "start": "pm2 start process.yml",
    "stop": "pm2 stop process.yml",
    "restart": "pm2 restart process.yml",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src test || exit 0",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "pending-local": "knex-migrate pending --env local",
    "migrate-local": "knex-migrate up --env local",
    "rollback-local": "knex-migrate rollback --env local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arielril/node-seed-di.git"
  },
  "keywords": [
    "node",
    "pm2",
    "i18n",
    "knex"
  ],
  "author": "Ariel Ril",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.9.0",
    "jest-json-schema": "^2.0.0",
    "joi": "^13.1.0",
    "knex": "^0.14.2",
    "knex-migrate": "^1.5.5",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "mysql2": "^1.5.1",
    "request": "^2.83.0",
    "request-debug": "^0.2.0",
    "supertest": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.1.0",
    "sinon": "^6.0.1"
  }
}
